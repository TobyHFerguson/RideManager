{
  "compilerOptions": {
    "target": "esnext",
    "noImplicitAny": true,
    "strict": true,
    "useUnknownInCatchVariables": true,
    "lib": [
      "esnext"
    ],
    "allowJs": true,
    "checkJs": true,
    "types": []
  },
  "include": [
    "**/*.js",
    "**/*.gs",
    "**/*.ts",
    "**/*.d.ts",
    "gas-globals.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}